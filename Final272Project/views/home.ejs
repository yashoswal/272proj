		<link rel="icon" href="images/favicon.ico">
		<link rel="shortcut icon" href="images/favicon.ico" />
		<link rel="stylesheet" href="css/style.css">
		<script src="js/jquery.js"></script>
		<script src="js/jquery-migrate-1.1.1.js"></script>
		<script src="js/bgStretch.js"></script>
		
		<script>
			$(document).ready(function(){
			$('#bgStretch')
				.bgStretch({
					align:'leftTop'
				})
			});
		</script>
	</head>
	<body class="">
		<div id="bgStretch"><img src="images/bg_body.jpg" alt=""></div>
<!--==============================header=================================-->
		<header> 		
			<div class="menu_block">
				<div class="container_12">
					<div class="grid_12"> 
						<nav class="" >
							<ul class="sf-menu">
								<li><a href="index-1.html">The Happening</a></li>
							</ul>
						</nav>
						<div class="clear"></div>
					</div>
					<div class="clear"></div>
				</div>
			</div>
		</header>
<!--=======content================================-->
		<div class="content_wrapper"><div class="ic"></div>
			<div class="container_12">
				<div class="grid_9">
					<div class="content">
						<div class="grid_5 alpha">
							<div class="block1">
								
								<div class="">
									<h3>Select the Category & Area for The Happening place</h3>
									<br/>
									<form name="happening" action="/happeningList" method="post">
									<table>
									<tbody>

									<tr>
										<td><label for="categories">Category:</label></td>
										<td><select id="categories" name="categories">
											  <option value="restaurants">Restaurants</option>
											  <option value="Cafes">Cafe</option>
											  <option value="bars">Bars & Lounges</option>
											  <option value="clubs">Night Clubs</option>
											  <option value="tourist attractions">Tourist Attractions</option>
											  <option value="events">Events</option>
										</select></td>
									</tr>
									<br/><br/>
									
									<tr><td/></tr>
									<tr>
									<td><label for="place">Place (County Name): </label> </td>
									<td><input id="place" maxlength="45" name="place" type="text" /> </td>
									</tr>
									<tr><td><input name="Submit" type="submit" value="Submit" /></td> </tr></tbody>
									</table>
									</form>
								</div>
								<div class="clear"></div>
							</div>
						</div>
						
						<div class="clear"></div>
						
					</div>
				</div>
				
			</div>
		</div>
